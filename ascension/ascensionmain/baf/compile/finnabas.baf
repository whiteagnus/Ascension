    SetGlobalTimer("Spell","LOCALS",6)
    SetGlobalTimer("VampiricTouch","LOCALS",300)
    ForceSpell(LastSeenBy(Myself),WIZARD_VAMPIRIC_TOUCH)
  RESPONSE #150
    Continue()
END

IF
  !Range(LastSeenBy(Myself),10)
  !GlobalTimerNotExpired("Running","LOCALS")
  OR(3)
  CheckStat(LastSeenBy(Myself),0,SCRIPTINGSTATE3)
  CheckStatLT(LastSeenBy(Myself),100,MAGICDAMAGERESISTANCE)
  CheckStatLT(LastSeenBy(Myself),100,RESISTSLASHING)
THEN
  RESPONSE #100
    MoveToObject(LastSeenBy(Myself))
END

IF
  StateCheck(Myself,STATE_INVISIBLE)
  HPPercentLT(Myself,40)
  !GlobalTimerNotExpired("Running","LOCALS")
THEN
  RESPONSE #100
    SetGlobalTimer("Running","LOCALS",5)
    RunAwayFrom(NearestEnemyOf(Myself),75)
END

IF
  Global("xyxManeuvered","LOCALS",0)
  GlobalTimerExpired("xyxAttack","LOCALS")
  !GlobalTimerNotExpired("Running","LOCALS")
THEN
  RESPONSE #100
    SetGlobal("xyxManeuvered","LOCALS",1)
    RandomWalkContinuous()
END

IF
  Range(LastSeenBy(Myself),10)
  !GlobalTimerNotExpired("Running","LOCALS")
  OR(3)
  CheckStat(LastSeenBy(Myself),0,SCRIPTINGSTATE3)
  CheckStatLT(LastSeenBy(Myself),100,MAGICDAMAGERESISTANCE)
  CheckStatLT(LastSeenBy(Myself),100,RESISTSLASHING)
THEN
  RESPONSE #100
    SetGlobalTimer("xyxAttack","LOCALS",5)
    SetGlobal("xyxManeuvered","LOCALS",0)
    Attack(LastSeenBy(Myself))
END

IF
  True()
THEN
  RESPONSE #100
    RandomWalkContinuous()
END

